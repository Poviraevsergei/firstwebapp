<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <filter>
    <filter-name>print-filter</filter-name>
    <filter-class>com.tms.filter.PrintRequestFilter</filter-class>
  </filter>

  <filter>
    <filter-name>cookie-filter</filter-name>
    <filter-class>com.tms.filter.GetCookieFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>print-filter</filter-name>
    <servlet-name>myfirstservlet</servlet-name>
  </filter-mapping>
  
  <filter-mapping>
    <filter-name>cookie-filter</filter-name>
    <servlet-name>myfirstservlet</servlet-name>
  </filter-mapping>
  
  <listener>
    <listener-class>com.tms.listener.RequestAttributeListener</listener-class>
  </listener>
</web-app>
